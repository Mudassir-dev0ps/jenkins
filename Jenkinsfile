pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                echo 'Building automatically because of SCM change'
            }
        }
 stage('Test') {
            steps {
                echo 'Testing new'
            }
        }
 stage('Deploy') {
            steps {
                echo 'Deploying new'
            }
        }
    }
}
